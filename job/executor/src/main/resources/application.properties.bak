# web port
server.port=8081
# no web
#spring.main.web-environment=false

# log config
logging.config=classpath:logback.xml


### xxl-jobEntity admin address list, such as "http://address" or "http://address01,http://address02"
xxl.jobEntity.admin.addresses=http://127.0.0.1:8080/xxl-jobEntity-admin

### xxl-jobEntity, access token
xxl.jobEntity.accessToken=default_token

### xxl-jobEntity executor appname
xxl.jobEntity.executor.appname=xxl-jobEntity-executor-sample
### xxl-jobEntity executor registry-address: default use address to registry , otherwise use ip:port if address is null
xxl.jobEntity.executor.address=
### xxl-jobEntity executor server-info
xxl.jobEntity.executor.ip=
xxl.jobEntity.executor.port=9999
### xxl-jobEntity executor log-path
xxl.jobEntity.executor.logpath=/data/applogs/xxl-jobEntity/jobhandler
### xxl-jobEntity executor log-retention-days
xxl.jobEntity.executor.logretentiondays=30
