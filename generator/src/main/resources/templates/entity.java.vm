package ${package.Entity};

#foreach($pkg in ${table.importPackages})
import ${pkg};
#end
#if(${isExtendsTimeEntity})
import ${EntityPackage}.${TimeEntityClassName}
#end
import lombok.AllArgsConstructor;
import lombok.NoArgsConstructor;
import lombok.Data;
import lombok.EqualsAndHashCode;

/**
 * <p>
 * $!{table.comment}
 * </p>
 *
 * @author ${author}
 * @since ${date}
 */
#if(${chainModel})
@Data
@Accessors(chain = true)
#end
#if(${table.convert})
@TableName("${schemaName}${table.name}")
#end
#if(${superEntityClass})
public class ${entity} extends ${superEntityClass}#if(${activeRecord})<${entity}>#end {
#elseif(${activeRecord})
public class ${entity} extends Model<${entity}> {
#elseif(${entitySerialVersionUID})
public class ${entity} implements Serializable #if(${isExtendsTimeEntity})extends ${timeEntityName}#end{
#else
public class ${entity} #if(${isExtendsTimeEntity})extends ${timeEntityName}#end{
#end
#if(${entitySerialVersionUID})

    private static final long serialVersionUID = 1L;
#end
## ----------  BEGIN 字段循环遍历  ----------
#foreach($field in ${table.fields})

#if(${field.keyFlag})
#set($keyPropertyName=${field.propertyName})
#end
#if("$!field.comment" != "")
    #if(${springdoc})
      @Schema(description = "${field.comment}")
    #elseif(${swagger})
      @ApiModelProperty("${field.comment}")
    #else
    #if(${isExtendsTimeEntity})
    #if(${field.name} != "create_time" && ${field.name} != "update_time")
    /**
     * ${field.comment}
     */
    #end
    #end
    #end
#end
#if(${field.keyFlag})
## 主键
    #if(${field.keyIdentityFlag})
    @TableId(value = "${field.annotationColumnName}", type = IdType.AUTO)
    #elseif(!$null.isNull(${idType}) && "$!idType" != "")
    @TableId(value = "${field.annotationColumnName}", type = IdType.${idType})
    #elseif(${field.convert})
    @TableId("${field.annotationColumnName}")
    #end
## 普通字段
#elseif(${field.fill})
## -----   存在字段填充设置   -----
    #if(${field.convert})
    @TableField(value = "${field.annotationColumnName}", fill = FieldFill.${field.fill})
    #else
    @TableField(fill = FieldFill.${field.fill})
    #end
#elseif(${field.convert})
    @TableField("${field.annotationColumnName}")
#end
## 乐观锁注解
#if(${field.versionField})
    @Version
#end
## 逻辑删除注解
#if(${field.logicDeleteField})
    @TableLogic
#end
    #if(${isExtendsTimeEntity})
    #if(${field.name} != "create_time" && ${field.name} != "update_time")
    private ${field.propertyType} ${field.propertyName};
    #end
    #else
    private ${field.propertyType} ${field.propertyName};
    #end
#end
## ----------  END 字段循环遍历  ----------
}
